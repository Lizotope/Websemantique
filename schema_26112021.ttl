# baseURI: http://example.org/tp
# TP1 - Websemantique

###########################################################

########## definitions de prefixes ########################
@prefix : <http://example.org/tp#> .
@prefix cc: <http://creativecommons.org/licences/> .
@prefix dbpedia-fr: <http://fr.dbpedia.org/resource/> .
@prefix dbpedia-owl: <http://dbpedia.org/ontology/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcmitype: <http://purl.org/dc/dcmitype/> .
@prefix dcterms: <http://dublincore.org/documents/dcmi-terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix p1: <http://www.geonames.org/countries/#> .
@prefix prop-fr: <http://fr.dbpedia.org/property/> .
@prefix prop-en: <http://dbpedia.org/property/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
#########################################################
#
dbpedia-owl:ArchitecturalStructure
  rdf:type owl:Class ;
  rdfs:subClassOf dbpedia-owl:Monument ;
.
dbpedia-owl:Monument
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
dbpedia-owl:Museum
  rdf:type owl:Class ;
  rdfs:subClassOf dbpedia-owl:Monument ;
.
dbpedia-owl:PopulatedPlace
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
dbpedia-owl:ReligiousBuilding
  rdf:type owl:Class ;
  rdfs:subClassOf dbpedia-owl:Monument ;
.
<http://example.org/perscido>
  rdf:type owl:Ontology ;
.
:Genre
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
:Photo
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
:anniversaire
  rdf:type :Genre ;
.
:famille
  rdf:type :Genre ;
.
:mer
  rdf:type :Genre ;
.
:montagne
  rdf:type :Genre ;
.
:paysage
  rdf:type :Genre ;
.
:shows
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Photo ;
  rdfs:range owl:Thing ;
.
prop-fr:ville
  rdf:type owl:ObjectProperty ;
  rdfs:domain dbpedia-owl:Monument ;
  rdfs:range dbpedia-owl:PopulatedPlace ;
.
prop-en:url
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Photo ;
  rdfs:range xsd:string;
.
dc:creator
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Photo ;
  rdfs:range foaf:Person ;
.
dbpedia-owl:owner
  rdf:type owl:ObjectProperty ;
  rdfs:domain dbpedia-fr:Album_photos ;
  rdfs:range foaf:Person ;
.
:AlbumParent
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Photo ;
  rdfs:range dbpedia-fr:Album_photos;
.
dc:date
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :Photo ;
  rdfs:range xsd:date ;
.
dc:description
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :Photo ;
  rdfs:range xsd:string ;
.
dc:type
  rdf:type rdf:Property ;
  rdfs:domain :Photo ;
  rdfs:range xsd:string ;
.
foaf:Person
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
foaf:firstName
  rdf:type owl:DatatypeProperty ;
  rdfs:domain foaf:Person ;
  rdfs:range xsd:string ;
.
foaf:lastName
  rdf:type owl:DatatypeProperty ;
  rdfs:domain foaf:Person ;
  rdfs:range xsd:string ;
.
#
