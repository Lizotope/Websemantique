######### instances de Monuments ########################
dbpedia-fr:Cathédrale_Notre-Dame_de_Paris
  rdf:type dbpedia-owl:ReligiousBuilding ;
  prop-fr:ville dbpedia-fr:Paris ;
.
dbpedia-fr:Musée_du_Louvre
  rdf:type dbpedia-owl:Museum ;
  prop-fr:ville dbpedia-fr:Paris ;
.
dbpedia-fr:Paris
  rdf:type dbpedia-owl:PopulatedPlace ;
.
dbpedia-fr:Tour_Eiffel
  rdf:type dbpedia-owl:ArchitecturalStructure ;
  prop-fr:ville dbpedia-fr:Paris ;
.
#
######## instances de personnes #########################
:JeanDupont
  foaf:firstName "Jean";
  foaf:lastName "Dupont";
.
#
######## instances d'album #############################
:Album1
  rdf:type dbpedia-fr:Album_photos;
  dbpedia-owl:owner :JeanDupont;
.
######## instances de photos ###########################
:Photo_1
  rdf:type :Photo ;
  :shows :anniversaire ;
  :shows dbpedia-fr:Cathédrale_Notre-Dame_de_Paris ;
  :AlbumParent :Album1;
  prop-fr:url "https://upload.wikimedia.org/wikipedia/commons/7/7e/Parvis_Notre_Dame_05%2C_850th_anniversary_of_Notre-Dame.jpg";
.
:Photo_2
  rdf:type :Photo ;
  :shows :anniversaire ;
  :shows dbpedia-fr:Cathédrale_Notre-Dame_de_Paris ;
.
